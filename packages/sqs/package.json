			"require": "./dist/*.js"
		}
	},
	"dependencies": {
		"@aws-sdk/client-sqs": "^3.848.0",
		"@sample/common": "workspace:*",
		"@sample/config": "workspace:*",
		"@nestjs/common": "^11.1.4",
		"@nestjs/config": "^4.0.2",
		"@nestjs/core": "^11.1.4"
	},
	"devDependencies": {
		"@sample/config-typescript": "workspace:*",
		"@types/node": "^24.0.14",
		"ts-loader": "^9.5.2",
		"ts-node": "^10.9.2",
		"typescript": "5.8.3"
	}
}
