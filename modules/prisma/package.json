{
	"name": "@sample/prisma",
	"version": "0.0.1",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"dev": "tsc -w",
		"build": "tsc",
		"clean": "rm -rf dist",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
		"test": "jest"
	},
	"main": "dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
